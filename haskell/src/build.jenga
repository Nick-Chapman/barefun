
executable-name:
  echo barefun > $@

include ~/code/jenga/rulelib/haskell.jenga

main-entry:
  echo CommandLine > $@

ghc-version:
  echo 9.8.4 > $@

ghc-options:
  echo -Wall >> $@
  echo -Werror >> $@
  echo -Wno-name-shadowing >> $@
  echo -O0 >> $@

default-extensions:
  echo DuplicateRecordFields >> $@
  echo GADTs >> $@
  echo LambdaCase >> $@
  echo NondecreasingIndentation >> $@

packages:
  echo unix >> $@
  echo bytestring >> $@
  echo text >> $@
